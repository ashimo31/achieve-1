<h1>Followers#index</h1>
<p>Find me in app/views/followers/index.html.erb</p>

<div class="container">
  <h2>フォローしている人</h2>
  <% @users.each do |follower| %>
      <div class="container-child col-md-2 col-sm-4 col-xs-6 ">
        <%= profile_img(follower) %>
        <%= follower.email %>
        <%= render 'follow_form', follower: follower %>
      </div>
  <% end %>
</div>
