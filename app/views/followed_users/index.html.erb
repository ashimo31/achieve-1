<div class="container">
  <h2>フォローされている人</h2>
  <% @followed_users.each do |user| %>
      <div class="container-child col-md-2 col-sm-4 col-xs-6 ">
        <%= profile_img(followed_user) %>
        <%= followed_user.email %>
        <%= render 'follow_form', followed_user: followed_user %>
      </div>
  <% end %>
</div>
